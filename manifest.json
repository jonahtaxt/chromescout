{
  "manifest_version": 2,

  "name": "Chromescout",
  "description": "A simple extension to get a quick glimpse of a Nightscout site",
  "version": "1.0",

  "browser_action": {
    "default_icon": "logo2.png",
    "default_popup": "popup.html",
	"default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}